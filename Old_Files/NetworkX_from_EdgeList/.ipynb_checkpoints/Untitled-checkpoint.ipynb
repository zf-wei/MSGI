   "source": [
    "import networkx as nx\n",
    "\n",
    "# Create a graph\n",
    "G = nx.Graph()\n",
    "G.add_edges_from([(1, 2), (2, 3), (3, 4), (4, 5), (5, 1)])\n",
    "\n",
    "# Define the output file path\n",
    "output_file = \"edgelist.txt\"\n",
    "\n",
    "# Write the edge list to the output file\n",
    "nx.write_edgelist(G, output_file, delimiter=' ', data=False)"
   ]
  }
